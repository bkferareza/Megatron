<script type="text/ng-template" id="items_renderer.html">
    <div ui-tree-handle ng-class="{ 'select': $treeScope.multiSelect, 'copy': $treeScope.copy }" class="treeNode" style="min-width:1000px">
        <a class="btn btn-success btn-xs" nodrag ng-click="toggle(this)">
            <span ng-show="showToggle(this)" class="glyphicon" ng-class="{ 'glyphicon-chevron-right': collapsed, 'glyphicon-chevron-down': !collapsed }"></span>
            <span ng-show="!showToggle(this)" class="glyphicon glyphicon-stop"></span>
        </a>
        <span class="glyphicon glyphicon-user" ng-if="account.Username != null"></span>
        <span class="glyphicon glyphicon-user" ng-if="account.Username == null"></span>
        <span class="glyphicon glyphicon-user" ng-if="account.Username == null"></span>
        <label class="userTree" ng-click="modify(this)">
            <span ng-if="account.AccessType == 1 && ((account.Username == 'defaultsuperadmin') || (account.UserId == 1))">
                [DSA]
            </span>
            <span ng-if="account.AccessType == 1 && ((account.Username != 'defaultsuperadmin') && (account.UserId > 1))">
                [SA]
            </span>
            <span ng-if="account.AccessType == 2">
                [A]
            </span>
            <span ng-if="account.AccessType == 3">
                [U]
            </span>
            {{account.Name}}
        </label>
    </div>
    <ol ui-tree-nodes ng-model="account.Child" ng-class="{ 'hidden': collapsed }">
        <li ng-repeat="account in account.Child"
            ui-tree-node ng-class="{ 'selected': selected, 'original': original }"
            ng-include="'items_renderer.html'" style="min-width: 50px;"></li>
    </ol>
</script>

<script type="text/ng-template" id="folders_renderer.html">
    <div ui-tree-handle ng-class="{ 'select': $treeScope.multiSelect, 'copy': $treeScope.copy }">
        <!--<a class="btn btn-success btn-xs" nodrag ng-click="toggle(this)" ng-show="showToggle(this)">
    <span class="glyphicon" ng-class="{ 'glyphicon-chevron-right': collapsed, 'glyphicon-chevron-down': !collapsed }"></span>
</a>-->
        <input type="checkbox" ng-model="item.access" ng-show="showAccess(this)" />
        <label ng-click="toggle(this)">
            {{item.title}}
        </label>
    </div>
    <ol ui-tree-nodes ng-model="item.items" ng-class="{ 'hidden': collapsed }">
        <li ng-repeat="item in item.items" ui-tree-node ng-class="{ 'selected': selected, 'original': original }" ng-include="'folders_renderer.html'"></li>
    </ol>
</script>

